    "dependencies": {
        "@commitlint/config-conventional": "latest",
        "@commitlint/types": "latest",
        "@types/eslint": "latest",
        "@types/lodash.merge": "latest",
        "@types/prettier": "latest",
        "@typescript-eslint/eslint-plugin": "latest",
        "@typescript-eslint/parser": "latest",
        "eslint-config-prettier": "latest",
        "eslint-define-config": "latest",
        "eslint-plugin-jest": "latest",
        "eslint-plugin-prettier": "latest",
        "lodash.merge": "latest",
        "stylelint-config-twbs-bootstrap": "latest"
    },
    "devDependencies": {
        "@commitlint/cli": "^16.0.2",
        "@release-it/conventional-changelog": "^4.1.0",
        "eslint": "^8.7.0",
        "prettier": "^2.5.1",
        "release-it": "^14.12.2",
        "stylelint": "^14.2.0",
        "typescript": "^4.5.4",
        "yorkie": "^2.0.0"
    },
    "peerDependencies": {
        "@commitlint/cli": ">= 16.0.0",
        "eslint": ">= 8.0.0",
        "prettier": ">= 2.0.0",
        "stylelint": ">= 14.0.0"
    }
}
