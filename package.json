{
    "name": "@techkit/linter-config",
    "version": "0.3.4",
    "license": "MIT",
    "description": "Configurations for linters",
    "repository": "git@github.com:techkit/linter-config.git",
    "contributors": [
        {
            "name": "Renzo Rosas",
            "url": "https://github.com/rrosas1198"
        }
    ],
    "publishConfig": {
        "access": "public"
    },
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.mjs",
            "require": "./dist/index.cjs"
        }
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "bauen",
        "release": "release-it"
    },
    "gitHooks": {
        "commit-msg": "commitlint -e -V"
    },
    "dependencies": {
        "@commitlint/config-conventional": "^17.0.0",
        "@commitlint/types": "^17.0.0",
        "@types/eslint": "^8.4.2",
        "@types/prettier": "^2.6.3",
        "@typescript-eslint/eslint-plugin": "^5.27.0",
        "@typescript-eslint/parser": "^5.27.0",
        "defu": "^6.0.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-define-config": "^1.5.0",
        "eslint-plugin-jest": "^26.4.6",
        "eslint-plugin-prettier": "^4.0.0",
        "stylelint-config-twbs-bootstrap": "^3.1.0"
    },
    "devDependencies": {
        "@commitlint/cli": "^17.0.1",
        "@release-it/conventional-changelog": "^5.0.0",
        "bauen": "^0.10.4",
        "eslint": "^8.16.0",
        "prettier": "^2.6.2",
        "release-it": "^15.0.0",
        "stylelint": "^14.8.5",
        "typescript": "^4.7.2",
        "yorkie": "^2.0.0"
    },
    "peerDependencies": {
        "@commitlint/cli": ">= 16.0.0",
        "eslint": ">= 8.0.0",
        "prettier": ">= 2.0.0",
        "stylelint": ">= 14.0.0"
    }
}
