{
    "name": "@techkit/linter-config",
    "version": "0.3.0",
    "license": "MIT",
    "description": "Configurations for linters",
    "repository": "git@github.com:techkit/linter-config.git",
    "contributors": [
        {
            "name": "Renzo Rosas",
            "url": "https://github.com/rrosas1198"
        }
    ],
    "publishConfig": {
        "access": "public"
    },
    "exports": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "tsc --build",
        "release": "release-it"
